{
    "name": "sendx/sendx-php-sdk",
    "description": "SendX is an email marketing product. It helps you convert website visitors to customers, send them promotional emails, engage with them using drip sequences and craft custom journeys using powerful but simple automations.",
    "keywords": [
        "php",
        "sdk",
        "rest",
        "api",
        "email",
        "marketing",
        "email_marketing",
        "sendx",
        "backend",
        "tool"
    ],
    "homepage": "https://sendx.io",
    "license": "MIT",
    "authors": [
        {
            "name": "SendX",
            "homepage": "https://sendx.io"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7.3",
        "guzzlehttp/psr7": "^1.7 || ^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 || ^9.0",
        "friendsofphp/php-cs-fixer": "^3.5"
    },
    "autoload": {
        "psr-4": { "sendx\\" : "src/" }
    },
    "autoload-dev": {
        "psr-4": { "sendx\\Test\\" : "test/" }
    }
}
